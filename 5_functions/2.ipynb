    "# Example 1 - Temprature conversion\n",
    "\n",
    "def convert_temp(temp,unit):\n",
    "    unit=unit.lower()\n",
    "    temp=int(temp)\n",
    "    if unit==\"c\":\n",
    "        return temp*9/5+32\n",
    "    elif unit==\"f\":\n",
    "        return (temp-32)*5/9\n",
    "    else:\n",
    "        return \"Invalid unit\"\n",
    "convert_temp(100,\"C\")\n"
   ]
